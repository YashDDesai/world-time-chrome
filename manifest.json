{
    "name": "World Clock",
    "version": "1.0",
    "description": "By Yash Desai",
    "permissions": ["activeTab", "storage"],
    "browser_action": {
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        },
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      }
  }